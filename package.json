    "@types/react-dom": "17.0.9",
    "@types/testing-library__cypress": "^5.0.9",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "autoprefixer": "^10.3.4",
    "babel-jest": "27.2.0",
    "babel-plugin-react-anonymous-display-name": "^0.1.0",
    "cross-env": "^7.0.3",
